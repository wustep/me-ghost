{{#if internal}}
  {{#foreach navigation}}
    {{#has label="Home, Projects, Articles, Ideas"}}
      <a class="navbar-item nav-{{slug}}{{#if current}} is-active{{/if}}" href="{{url absolute="true"}}">
          {{label}}
      </a>
    {{/has}}
  {{/foreach}}
{{else}}
  {{#foreach navigation}}
    {{^has label="Home, Projects, Articles, Ideas"}}
      <a class="navbar-item nav-{{slug}}" href="{{url absolute="true"}}">
          {{label}}
      </a>
    {{/has}}
  {{/foreach}}
{{/if}}
