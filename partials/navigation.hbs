{{#if internal}}
  {{#foreach navigation}}
    {{#has label="Home, Articles, Projects, Notes"}}
      <a class="navbar-item nav-{{slug}}{{#if current}} is-active{{/if}}" href="{{url absolute="true"}}">
        {{label}}
      </a>
    {{/has}}
  {{/foreach}}
{{else}}
  {{#foreach navigation}}
    {{^has label="Home, Articles, Projects, Notes"}}
      <a class="navbar-item nav-{{slug}}" href="{{url absolute="true"}}">
        {{#has label="Facebook"}}
          {{> icons/facebook}}
        {{/has}}
        {{#has label="Twitter"}}
          {{> icons/twitter}}
        {{/has}}
        {{#has label="LinkedIn"}}
          {{> icons/linkedin}}
        {{/has}}
        {{#has label="GitHub"}}
          {{> icons/github}}
        {{/has}}
        {{label}}
      </a>
    {{/has}}
  {{/foreach}}
{{/if}}
